{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Warning",
        "System.Net.Http.HttpClient": "Warning",
        "Polly": "Warning"
      }
    }
  },
  "Application": {
    "DocumentDeletionManagement": {
      "EnabledForRsins": "", // Note: add a point-comma (;) separated list of rsin's for which delettion takes place
      "PollingInterval": "00:10:00",
      "OlderThanDuration": "02:00:00",
      "BatchSize": 100
    }
  },
  "Hangfire": {
    "ScheduledRetries": [ "0.00:15", "0.00:30", "0.01:00", "0.04:00", "1.00:00" ]
  },
  "Auth": {
    "Authority": "https://keycloak-tool.oneground.local/realms/OneGround/",
    "ValidIssuer": "https://keycloak-tool.oneground.local/realms/OneGround",
    "ValidAudience": "account"
  }
}
